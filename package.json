{
  "name": "@dbozhinovski/chakra-ui-reasonable-colors",
  "version": "1.0.0",
  "description": "A Chakra UI component that integrates the reasonable colors palette for easier accessibility (https://reasonable.work/colors/)",
  "main": "./dist/vite-ts-lib-starter.cjs",
  "module": "./dist/vite-ts-lib-starter.mjs",
  "exports": {
    ".": {
      "require": "./dist/vite-ts-lib-starter.cjs",
      "import": "./dist/vite-ts-lib-starter.mjs"
    }
  },
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build && dts-bundle-generator --config ./dts-bundle-generator.config.ts",
    "test": "jest --runInBand",
    "test:coverage": "jest --runInBand --coverage",
    "lint:scripts": "eslint . --ext .ts",
    "lint:styles": "stylelint ./**/*.{css,scss}",
    "format:scripts": "prettier . --write",
    "format:styles": "stylelint ./**/*.{css,scss} --fix",
    "format": "npm run format:scripts && npm run format:styles",
    "prepare": "husky install && husky add .husky/pre-commit 'npx lint-staged' && git add .husky/pre-commit"
  },
  "devDependencies": {
    "@chakra-ui/react": "latest",
    "@chakra-ui/system": "latest",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@types/jest": "^29.0.0",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.11.5",
    "@types/react": "^18.0.23",
    "@types/react-dom": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "@vitejs/plugin-react": "^2.1.0",
    "dts-bundle-generator": "^6.13.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "framer-motion": "^6.5.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint": "^14.11.0",
    "stylelint-config-recommended": "^9.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.0"
  },
  "peerDependencies": {
    "@chakra-ui/react": ">=2.0.0",
    "@chakra-ui/system": ">=2.0.0",
    "react": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DBozhinovski/chakra-ui-reasonable-colors.git"
  },
  "keywords": [
    "react",
    "chakra-ui",
    "theme",
    "reasonable-colors"
  ],
  "author": "Darko Bozhinovski <hello@darko.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DBozhinovski/chakra-ui-reasonable-colors/issues"
  },
  "homepage": "https://github.com/DBozhinovski/chakra-ui-reasonable-colors#readme"
}
